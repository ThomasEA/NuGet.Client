  <Target Name="AfterBuild">
    <PropertyGroup>
      <PublishDestination>$(ArtifactRoot)\$(VsixOutputDirName)</PublishDestination>
    </PropertyGroup>
    <ItemGroup>
      <PackageManifest Include="$(OutputPath)Microsoft.VisualStudio.NuGet.Core.json" />
    </ItemGroup>
    <Message Text="$(TargetVsixContainer) -&gt; $(PublishDestination)" Importance="high" />
    <Copy SourceFiles="$(TargetVsixContainer)" DestinationFolder="$(PublishDestination)" OverwriteReadOnlyFiles="true" />
    <Message Text="@(PackageManifest) -&gt; $(PublishDestination)" Importance="high" />
    <Copy SourceFiles="@(PackageManifest)" DestinationFolder="$(PublishDestination)" OverwriteReadOnlyFiles="true" />
  </Target>
  
